<?xml version="1.0"?>
<config>
    <modules>
        <Tradiv_License>
            <version>0.1.0</version>
        </Tradiv_License>
    </modules>
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                    <customer_grid>Tradiv_License_Block_Adminhtml_Customer_Grid</customer_grid>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <tradiv_license>
                <class>Tradiv_License_Helper</class>
            </tradiv_license>
        </helpers>
        <resources>
            <tradiv_license_setup>
                <setup>
                    <module>Tradiv_License</module>
                </setup>
            </tradiv_license_setup>
        </resources>
    </global>
    <frontend>
        <layout>
            <updates>
                <tradiv_license>
                    <file>tradiv_license_customer.xml</file>
                </tradiv_license>
            </updates>
        </layout>
    </frontend>
</config>