<?xml version="1.0"?>
<config>
    <modules>
        <Tradiv_License>
            <version>0.1.0</version>
        </Tradiv_License>
    </modules>
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                    <customer_grid>Tradiv_License_Block_Adminhtml_Customer_Grid</customer_grid>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <tradiv_license>
                <class>Tradiv_License_Helper</class>
            </tradiv_license>
        </helpers>
        <resources>
            <tradiv_license_setup>
                <setup>
                    <module>Tradiv_License</module>
                    <class>Mage_Customer_Model_Entity_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </tradiv_license_setup>
            <tradiv_license_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </tradiv_license_write>
            <tradiv_license_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </tradiv_license_read>
        </resources>
    </global>
</config>